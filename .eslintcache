 from './Routes'\nimport Footer from '../components/template/Footer'\n\nexport default props =>\n    <BrowserRouter>\n        <div className=\"app\">\n            <Logo />\n            <Nav />\n            <Routes />\n            <Footer />\n        </div>\n    </BrowserRouter>","/home/gabrielbagalho/Documentos/teste-EQi/exemplo/frontend-eqi/src/main/Routes.jsx",["60"],"import React from 'react'\nimport { Switch, Route, Redirect } from 'react-router'\n\nimport Home from '../components/home/Home'\nimport UserCrud from '../pages/user/userCrud'\nimport InvestmentCrud from '../pages/investment/investmentCrud'\n\nexport default props => \n    <Switch>\n        <Route exact path='/' component={Home} />\n        <Route path='/users' component={UserCrud} />\n        <Route path='/investment' component={InvestmentCrud} />\n        <Redirect from='*' to='/' />\n    </Switch>","/home/gabrielbagalho/Documentos/teste-EQi/exemplo/frontend-eqi/src/components/template/Logo.jsx",["61"],"import './Logo.css'\nimport logo from '../../assets/imgs/logo.png'\nimport React from 'react'\nimport { Link } from 'react-router-dom'\n\nexport default props =>\n    <aside className=\"logo\">\n        <Link to=\"/\" className=\"logo\">\n            <img src={logo} alt=\"logo\" />\n        </Link>\n    </aside>","/home/gabrielbagalho/Documentos/teste-EQi/exemplo/frontend-eqi/src/components/template/Footer.jsx",["62","63"],"import './Footer.css'\nimport React from 'react'\n\n// eslint-disable-next-line import/no-anonymous-default-export\nexport default props =>\n    <footer className=\"footer\">\n        <span id=\"link\">\n            <a href=\"http://linkedin.com/in/gabriel-de-oliveira-bagalho-56b906152\" target=\"_blank\">Gabriel Bagalho</a>\n        </span>\n        <span id=\"git\">\n            <a href=\"http://github.com/gabrielbagalho\" target=\"_blank\">GitHub</a>\n        </span>\n    </footer>","/home/gabrielbagalho/Documentos/teste-EQi/exemplo/frontend-eqi/src/components/template/Nav.jsx",["64"],"import './Nav.css'\nimport React from 'react'\nimport { Link } from 'react-router-dom'\n\nexport default props =>\n    <aside className=\"menu-area\">\n        <nav className=\"menu\">\n            <Link to=\"/\">\n                <i className=\"fa fa-home\"></i> Início\n            </Link>\n            <Link to=\"/users\">\n                <i className=\"fa fa-user\"></i> Cadastro\n            </Link>\n            <Link to=\"/investment\">\n                <i className=\"fa fa-handshake-o\"></i> Simulador de Investimentos\n            </Link>\n        </nav>\n    </aside>","/home/gabrielbagalho/Documentos/teste-EQi/exemplo/frontend-eqi/src/components/home/Home.jsx",["65"],"import React from 'react'\nimport Main from '../template/Main'\n\n\nexport default props =>\n    <Main icon=\"home\" title=\"EQI Investimentos\"\n        subtitle=\"Olá, use à vontade, feito para você! \">\n        <hr />\n       \n        <p id=\"text\"><strong>Esse simulador é para você comparar e decidir como insvetir melhor o seu dinheiro.</strong></p>\n        \n        <div className=\"contents-cdi\">\n        \n        <strong id=\"title-cdi\">Taxa CDI : </strong>\n        <p id=\"text-cdi\"> O CDI é um dos principais índices do mercado financeiro. Ele indica, na maioria das vezes, o rendimento de diversos investimentos e varia mês a mês. Quando falamos do CDI 2020, portanto, estamos falando do CDI acumulado ao longo deste ano, somando os resultados que ele apresenta em cada mês. </p>\n        </div>\n        \n        <div className=\"contents-saving\">\n\n        <strong id=\"title-saving\">Taxa Poupança: </strong>\n        <p id=\"saving-text\">Muita gente não considera que a poupança seja um investimento de fato. Mas na prática, ela normalmente é o primeiro contato da maioria das pessoas com as aplicações disponíveis no mercado financeiro. Cerca de 67 milhões de brasileiros têm pelo menos. A poupança é uma aplicação de renda fixa simples. Vale destacar que a rentabilidade da poupança é a mesma em qualquer instituição. Portanto, a escolha do banco não vai influenciar no retorno do investimento.</p>\n\n        </div>\n    </Main>\n\n        ","/home/gabrielbagalho/Documentos/teste-EQi/exemplo/frontend-eqi/src/pages/user/userCrud.jsx",[],"/home/gabrielbagalho/Documentos/teste-EQi/exemplo/frontend-eqi/src/pages/investment/investmentCrud.jsx",["66"],"import React, { Component } from 'react'\nimport axios from 'axios'\nimport Main from '../../components/template/Main'\n\nconst headerProps = {\n    title: \n        <strong>Simulador de investimentos</strong>\n}\n\nconst baseUrl = 'http://localhost:3001'\nconst baseUrlInvestment = 'http://localhost:3001/investment-parameters'\nconst initialState = {\n    investment: {  id_user: '' , value: '', investment_time: '', rate_cdi_percent: ''},\n    list: []\n}\n\nexport default class InvestmentCrud extends Component {\n\n    state = { ...initialState }\n\n    clear() {\n        this.setState({ investment: initialState.investment })\n    }\n\n    save() {\n        const investment = this.state.investment\n        const method = investment.id ? 'put' : 'post'\n        const url = investment.id ? `${baseUrlInvestment}/${investment.id}` : baseUrlInvestment\n        axios[method](url, investment)\n            .then(resp => {\n                const list = this.getUpdatedList(resp.data)\n                this.setState({ investment: initialState.investment, list })\n            })\n    }\n    \n    getUpdatedList(investment, add = true) {\n        const list = this.state.list.filter(u => u.id !== investment.id)\n        if(add) list.unshift(investment)\n        return list\n    }\n\n    updateField(event) {\n        const investment = { ...this.state.investment }\n        investment[event.target.name] = event.target.value\n        this.setState({ investment: investment })\n    }\n\n    renderForm() {\n        return (\n            <div className=\"form\">\n                <div className=\"row\">\n                    <div className=\"col-12 col-md-6\">\n                        <div className=\"form-group\">\n                            <label><strong>Usúario</strong></label>\n                            <input type=\"text\" className=\"form-control\"\n                                name=\"id_user\"\n                                value={this.state.investment.id_user}\n                                onChange={e => this.updateField(e)}\n                                placeholder=\"Digite o Numero indentificador do usuario...\" />\n                        </div>\n                    </div>\n                    <div className=\"col-12 col-md-6\">\n                        <div className=\"form-group\">\n                            <label><strong>Valor a ser investido</strong> </label>\n                            <input type=\"text\" className=\"form-control\"\n                                name=\"value\"\n                                value={this.state.investment.value}\n                                onChange={e => this.updateField(e)}\n                                placeholder=\"Digite o valor...\" />\n                        </div>\n                    </div>\n\n                    <div className=\"col-12 col-md-6\">\n                        <div className=\"form-group\">\n                            <label><strong>Tempo do investimento em anos</strong></label>\n                            <input type=\"text\" className=\"form-control\"\n                                name=\"investment_time\"\n                                value={this.state.investment.investment_time}\n                                onChange={e => this.updateField(e)}\n                                placeholder=\"Digite o tempo do investimento...\" />\n                        </div>\n                    </div>\n                    <div className=\"col-12 col-md-6\">\n                        <div className=\"form-group\">\n                            <label><strong>Digite a % de CDI no investimento CDB</strong></label>\n                            <input type=\"text\" className=\"form-control\"\n                                name=\"rate_cdi_percent\"\n                                value={this.state.investment.rate_cdi_percent}\n                                onChange={e => this.updateField(e)}\n                                placeholder=\"Digite a % de CDI no investimento CDB...\" />\n                        </div>\n                    </div>\n                </div>\n\n                <hr />\n                <div className=\"row\">\n                    <div className=\"col-12 d-flex justify-content-end\">\n                        <button className=\"btn btn-primary\"\n                            onClick={e => this.save(e)}>\n                            Salvar\n                        </button>\n\n                        <button className=\"btn btn-secondary ml-2\"\n                            onClick={e => this.clear(e)}>\n                            Cancelar\n                        </button>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n\n    load(investment) {\n        this.setState({ investment })\n    }\n\n    remove(investment) {\n        axios.delete(`${baseUrlInvestment}/${investment.id}`).then(resp => {\n            const list = this.getUpdatedList(investment, false)\n            this.setState({ list })\n        })\n    }\n\n    renderTable() {\n        return (\n            <table className=\"table mt-4\">\n                <thead>\n                    <tr>\n                        <th>Usúario</th>\n                        <th>Investimento Inicial</th>\n                        <th>Rendimento na Poupança</th>\n                        <th>Rendimento no CDB</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {this.renderRows()}\n                </tbody>\n            </table>\n        )\n    }\n\n    renderRows() {\n        return this.state.list.map(investment => {\n            return (\n                <tr key={investment.id_user}>\n                    <td>{investment.id_user}</td>\n                    <td>{investment.value}</td>\n                    <td>{investment.profitability_poupanca}</td>\n                    <td>{investment.profitability_cdb}</td>\n                    <td>\n                        <button className=\"btn btn-warning\"\n                            onClick={() => this.load(investment)}>\n                            <i className=\"fa fa-pencil\"></i>\n                        </button>\n                        <button className=\"btn btn-danger ml-2\"\n                            onClick={() => this.remove(investment)}>\n                            <i className=\"fa fa-trash\"></i>\n                        </button>\n                    </td>\n                </tr>\n            )\n        })\n    }\n    \n    render() {\n        return (\n            <Main {...headerProps}>\n                {this.renderForm()}\n                {this.renderTable()}\n            </Main>\n        )\n    }\n}","/home/gabrielbagalho/Documentos/teste-EQi/exemplo/frontend-eqi/src/components/template/Main.jsx",["67"],"import './Main.css'\nimport React from 'react'\nimport Header from './Header'\n\nexport default props =>\n    <React.Fragment>\n        <Header {...props} />\n        <main className=\"content container-fluid\">\n            <div className=\"p-3 mt-3\">\n                {props.children}\n            </div>\n        </main>\n    </React.Fragment>","/home/gabrielbagalho/Documentos/teste-EQi/exemplo/frontend-eqi/src/components/template/Header.jsx",["68"],"import './Header.css'\nimport React from 'react'\n\nexport default props =>\n    <header className=\"header d-none d-sm-flex flex-column\">\n        <h1 className=\"mt-3\">\n            <i className={`fa fa-${props.icon}`}></i> {props.title}\n        </h1>\n        <p className=\"lead text-muted\">{props.subtitle}</p>\n    </header>",["69","70"],{"r